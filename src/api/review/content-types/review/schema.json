{
  "kind": "collectionType",
  "collectionName": "reviews",
  "info": {
    "singularName": "review",
    "pluralName": "reviews",
    "displayName": "Review",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "create_at": {
      "type": "datetime"
    },
    "rating": {
      "type": "decimal",
      "min": 0,
      "max": 5,
      "required": true,
      "default": 0
    },
    "headline": {
      "type": "string",
      "required": true
    },
    "comment": {
      "type": "string",
      "required": true
    },
    "products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "inversedBy": "reviews"
    },
    "user_detail": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::user-detail.user-detail",
      "inversedBy": "reviews"
    }
  }
}
