{
  "kind": "collectionType",
  "collectionName": "carts",
  "info": {
    "singularName": "cart",
    "pluralName": "carts",
    "displayName": "Cart",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "guest_users": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::guest-user.guest-user",
      "mappedBy": "cart"
    },
    "uuid": {
      "type": "customField",
      "regex": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$",
      "required": true,
      "customField": "plugin::field-uuid.uuid"
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "cart"
    },
    "product_details": {
      "displayName": "product-quantity",
      "type": "component",
      "repeatable": true,
      "component": "cart.product-quantity"
    }
  }
}
