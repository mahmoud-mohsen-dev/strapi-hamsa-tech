  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "price": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "decimal",
      "min": 0,
      "default": 0
