      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "average_reviews": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "decimal",
      "min": 0,
      "default": 0
